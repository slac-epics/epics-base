+ErConfigure( 0, 0, 0, 0, 1 )
+
+## Load record instances
+dbLoadRecords( "db/iocAdmin.db",			"IOC=_IOCPVROOT_" )
+dbLoadRecords( "db/save_restoreStatus.db",	"IOC=_IOCPVROOT_" )
+dbLoadRecords( "db/evr.db",					"IOC=_IOCPVROOT_,EVR=_EVRPVROOT_" )
