+dbLoadRecords("db/dbSubExample.db","user=_USER_Host")
+
+## Set this to see messages from mySub
+#var mySubDebug 1
+
+cd ${TOP}/iocBoot/${IOC}
+iocInit()
+
+## Start any sequence programs
+#seq sncExample,"user=_USER_Host"
+
