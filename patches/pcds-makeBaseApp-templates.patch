+	{ $(IOC) , IOCMANAGERS }
+}
+
+file ioc.db
+{
+	pattern
+	{ IOCNAME }
+	{ $(IOC)  }
+}
+
+file iocCluster.db
+{
+#                   Pool   Size
+	pattern
+	{ IOCNAME ,  P   ,  S   , TYPE   }
